{
  "name": "spinnies",
  "version": "0.5.0",
  "description": "Create and manage multiple spinners in command-line interface programs",
  "main": "index.js",
  "scripts": {
    "test": "npx mocha test",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
